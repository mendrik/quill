{
    "name": "quill-frontend",
    "version": "1.0.0",
    "description": "Frontend module for Quill",
    "main": "index.js",
    "scripts": {
        "start": "parcel src/typescript/start.tsx -d ../target/web/public/main -o quill.js --no-hmr --no-autoinstall",
        "build": "parcel build src/typescript/start.tsx  -d ../public -o quill.js"
    },
    "author": "Andreas Herd",
    "license": "ISC",
    "dependencies": {
        "@mdi/font": "2.4.85",
        "bulma": "0.7.1",
        "preact": "8.2.9",
        "preact-compat": "3.18.0",
        "preact-quill": "1.0.0"
    },
    "devDependencies": {
        "@types/node": "10.1.2",
        "autoprefixer": "8.5.0",
        "cross-env": "5.1.6",
        "cssnano": "3.10.0",
        "npm-run-all": "4.1.3",
        "postcss": "6.0.22",
        "postcss-cssnext": "3.1.0",
        "postcss-custom-properties": "7.0.0",
        "postcss-import": "11.1.0",
        "postcss-mixins": "6.2.0",
        "postcss-nested": "3.0.0",
        "postcss-parcel-import": "0.0.2",
        "sinon": "5.0.10",
        "stylelint": "9.2.1",
        "ts-node": "6.0.5",
        "tslint": "5.10.0",
        "typescript": "2.8.3"
    },
    "alias": {
        "react": "preact-compat",
        "react-dom": "preact-compat"
    }
}
